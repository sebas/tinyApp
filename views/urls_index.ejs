<!DOCTYPE html>
<html lang="en">
<head>
  <title>TinyApp: URLs list</title>
</head>
<body>
    <header>
        <% include ../views/partials/_header %>
    </header>

    <h1>URLS:</h1>

    <ul>
        <% for (const key in urls) { %>
            <% if (urls.hasOwnProperty(key)) { %>
                <% const url = urls[key]; %>
                <li><a href="/u/<%= key %>"><%= key %></a> -> <a href="<%= url.longURL %>"><%= url.longURL %></a> <a href="/urls/<%= key %>">Edit</a> <form method="POST" action="/urls/<%= key %>/delete"><input type="submit" value="Delete"></form></li>
            <% } %>
        <% } %>
    </ul>
</body>
</html>
<!-- urls_index.ejs -->